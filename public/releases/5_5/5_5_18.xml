  <announcements>
    <announcement lang="en"/>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(Incorrect last used array index copied to new array after unset).</description>
          <references>
            <reference type="bugfix">67985</reference>
          </references>
        </change>
        <change>
          <description>(Windows 8.1/Server 2012 R2 OS build number reported as 6.2 (instead of 6.3)).</description>
          <references>
            <reference type="bugfix">67739</reference>
          </references>
        </change>
        <change>
          <description>(A foreach on an array returned from a function not doing copy-on-write).</description>
          <references>
            <reference type="bugfix">67633</reference>
          </references>
        </change>
        <change>
          <description>(proc_open on Windows hangs forever).</description>
          <references>
            <reference type="bugfix">51800</reference>
          </references>
        </change>
        <change>
          <description>(Integer overflow in unserialize() (32-bits only)). (CVE-2014-3669)</description>
          <references>
            <reference type="bugfix">68044</reference>
            <reference type="cve">CVE-2014-3669</reference>
          </references>
        </change>
      </module>
      <module id="curl">
        <change>
          <description>(NULL byte injection - cURL lib).</description>
          <references>
            <reference type="bugfix">68089</reference>
          </references>
        </change>
      </module>
      <module id="exif">
        <change>
          <description>(Heap corruption in exif_thumbnail()). (CVE-2014-3670)</description>
          <references>
            <reference type="bugfix">68113</reference>
            <reference type="cve">CVE-2014-3670</reference>
          </references>
        </change>
      </module>
      <module id="fpm">
        <change>
          <description>(PHP-FPM incorrectly defines the SCRIPT_NAME variable when using Apache, mod_proxy-fcgi and ProxyPass).</description>
          <references>
            <reference type="bugfix">65641</reference>
          </references>
        </change>
      </module>
      <module id="openssl">
        <change>
          <description>Revert regression introduced by fix of bug [#41631](http://bugs.php.net/41631).</description>
          <references/>
        </change>
      </module>
      <module id="reflection">
        <change>
          <description>(Duplicate entry in Reflection for class alias).</description>
          <references>
            <reference type="bugfix">68103</reference>
          </references>
        </change>
      </module>
      <module id="session">
        <change>
          <description>(SessionHandler Invalid memory read create_sid()).</description>
          <references>
            <reference type="bugfix">67972</reference>
          </references>
        </change>
      </module>
      <module id="xmlrpc">
        <change>
          <description>(Global buffer overflow in mkgmtime() function). (CVE-2014-3668)</description>
          <references>
            <reference type="bugfix">68027</reference>
            <reference type="cve">CVE-2014-3668</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <windows_builds/>
  <tags/>
</release>
