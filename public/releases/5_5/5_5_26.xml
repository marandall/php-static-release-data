<?xml version="1.0"?>
<release version="5.5.26">
  <version>5.5.26</version>
  <date>2015-06-11</date>
  <stage>stable</stage>
  <source>
    <file compression="tar.bz2">
      <name>PHP 5.5.26 (tar.bz2)</name>
      <filename>php-5.5.26.tar.bz2</filename>
      <date>2015-06-11</date>
