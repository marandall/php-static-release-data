  <announcements>
    <announcement lang="en">The PHP development team announces the immediate availability of PHP 7.2.6. This is a primarily a bugfix release which includes a memory corruption fix for EXIF.</announcement>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(exif_read_data zend_mm_heap corrupted).</description>
          <references>
            <reference type="bugfix">76164</reference>
          </references>
        </change>
      </module>
      <module id="fpm">
        <change>
          <description>--with-fpm-acl wrongly tries to find libacl on FreeBSD.</description>
          <references>
            <reference type="bugfix">76075</reference>
          </references>
        </change>
      </module>
      <module id="intl">
        <change>
          <description>(Locale::parseLocale() broken with some arguments).</description>
          <references>
            <reference type="bugfix">74385</reference>
          </references>
        </change>
      </module>
      <module id="opcache">
        <change>
          <description>(PHP-FPM sporadic crash when running Infinitewp).</description>
          <references>
            <reference type="bugfix">76205</reference>
          </references>
        </change>
        <change>
          <description>(Assertion failure in file cache when unserializing empty try_catch_array).</description>
          <references>
            <reference type="bugfix">76275</reference>
          </references>
        </change>
        <change>
          <description>(Opcache causes incorrect "undefined variable" errors).</description>
          <references>
            <reference type="bugfix">76281</reference>
          </references>
        </change>
      </module>
      <module id="reflection">
        <change>
          <description>Fixed arginfo of array_replace(_recursive) and array_merge(_recursive).</description>
          <references/>
        </change>
      </module>
      <module id="session">
        <change>
          <description>(Url Rewriting (trans_sid) not working on urls that start with "#").</description>
          <references>
            <reference type="bugfix">74892</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <tags/>
</release>
