    </file>
    <file compression="tar.gz">
      <name>PHP 7.2.23 (tar.gz)</name>
      <filename>php-7.2.23.tar.gz</filename>
      <date>2019-09-26</date>
      <sha256>b32b426c84ff45154d6c11f00aff433bcac831a5c0a09bf0297075eefaea8fcc</sha256>
    </file>
    <file compression="tar.xz">
      <name>PHP 7.2.23 (tar.xz)</name>
      <filename>php-7.2.23.tar.xz</filename>
      <date>2019-09-26</date>
      <sha256>74e045ec8ff26290db6a3688826dcdf43b87bc509e508e9cb76dab742804ca14</sha256>
