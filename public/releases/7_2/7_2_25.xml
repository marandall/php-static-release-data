    <source>
      <name>PHP 7.2.25 (tar.xz)</name>
      <filename>php-7.2.25.tar.xz</filename>
      <date>2019-11-21</date>
      <sha256>746efeedc38e6ff7b1ec1432440f5fa801537adf6cd21e4afb3f040e5b0760a9</sha256>
