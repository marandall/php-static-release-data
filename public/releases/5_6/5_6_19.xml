  <announcements>
    <announcement lang="en"/>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(Built-in HTTP server, we can download file in web by bug).</description>
          <references>
            <reference type="bugfix">71559</reference>
          </references>
        </change>
      </module>
      <module id="curl">
        <change>
          <description>(Copied handle with new option CURLOPT_HTTPHEADER crashes while curl_multi_exec).</description>
          <references>
            <reference type="bugfix">71523</reference>
          </references>
        </change>
      </module>
      <module id="date">
        <change>
          <description>(Datetime comparisons ignore microseconds).</description>
          <references>
            <reference type="bugfix">68078</reference>
          </references>
        </change>
        <change>
          <description>(Calls to date_modify will mutate timelib_rel_time, causing date_date_set issues).</description>
          <references>
            <reference type="bugfix">71525</reference>
          </references>
        </change>
      </module>
      <module id="fileinfo">
        <change>
          <description>(finfo throws notice for specific python file).</description>
          <references>
            <reference type="bugfix">71434</reference>
          </references>
        </change>
      </module>
      <module id="fpm">
        <change>
          <description>(FPM not working with Apache httpd 2.4 balancer/fcgi setup).</description>
          <references>
            <reference type="bugfix">62172</reference>
          </references>
        </change>
      </module>
      <module id="opcache">
        <change>
          <description>(Possible use-after-free of ZCG(cwd) in Zend Opcache).</description>
          <references>
            <reference type="bugfix">71584</reference>
          </references>
        </change>
      </module>
      <module id="pdo mysql">
        <change>
          <description>(#70389 fix causes segmentation fault).</description>
          <references>
            <reference type="bugfix">71569</reference>
          </references>
        </change>
      </module>
      <module id="phar">
        <change>
          <description>(Out-of-Bound Read in phar_parse_zipfile()).</description>
          <references>
            <reference type="bugfix">71498</reference>
          </references>
        </change>
      </module>
      <module id="standard">
        <change>
          <description>(strip_tags improper php code parsing).</description>
          <references>
            <reference type="bugfix">70720</reference>
          </references>
        </change>
      </module>
      <module id="wddx">
        <change>
          <description>(Use-After-Free / Double-Free in WDDX Deserialize).</description>
          <references>
            <reference type="bugfix">71587</reference>
          </references>
        </change>
      </module>
      <module id="xsl">
        <change>
          <description>(NULL pointer dereference in xsl_ext_function_php()).</description>
          <references>
            <reference type="bugfix">71540</reference>
          </references>
        </change>
      </module>
      <module id="zip">
        <change>
          <description>(NULL pointer dereference in Zip::ExtractTo).</description>
          <references>
            <reference type="bugfix">71561</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <tags/>
</release>
