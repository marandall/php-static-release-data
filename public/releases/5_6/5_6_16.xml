          <references>
            <reference type="bugfix">69757</reference>
          </references>
        </change>
      </module>
      <module id="soap">
        <change>
          <description>(Segmentation fault if wsdl has no targetNamespace attribute).</description>
          <references>
            <reference type="bugfix">70875</reference>
          </references>
        </change>
      </module>
      <module id="spl">
