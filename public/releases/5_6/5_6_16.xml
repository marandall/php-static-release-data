          <references>
            <reference type="bugfix">68344</reference>
          </references>
        </change>
      </module>
      <module id="oci8">
        <change>
          <description>(OCI int overflow).</description>
          <references>
            <reference type="bugfix">68298</reference>
          </references>
        </change>
      </module>
      <module id="pdo_dblib">
