          <references>
            <reference type="bugfix">63447</reference>
          </references>
        </change>
      </module>
      <module id="oci8">
        <change>
          <description>(Add ORA-00028 to the PHP_OCI_HANDLE_ERROR macro)</description>
          <references>
            <reference type="bugfix">63265</reference>
          </references>
        </change>
      </module>
      <module id="pcre">
        <change>
          <description>(Corruption of hash tables).</description>
          <references>
            <reference type="bugfix">63180</reference>
          </references>
        </change>
        <change>
          <description>(Segfault in zend_gc with SF2 testsuite).</description>
          <references>
            <reference type="bugfix">63055</reference>
          </references>
        </change>
        <change>
          <description>(Upgrade PCRE to 8.31).</description>
          <references>
            <reference type="bugfix">63284</reference>
          </references>
        </change>
      </module>
