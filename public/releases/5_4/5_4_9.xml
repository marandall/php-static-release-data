          <references>
            <reference type="bugfix">63248</reference>
          </references>
        </change>
      </module>
      <module id="libxml">
        <change>
          <description>(Missing context check on libxml_set_streams_context() causes memleak).</description>
          <references>
            <reference type="bugfix">63389</reference>
          </references>
        </change>
      </module>
