<?xml version="1.0"?>
<release version="5.4.11">
  <version>5.4.11</version>
  <date>2013-01-17</date>
  <stage>stable</stage>
  <source>
    <file compression="tar.bz2">
      <name>PHP 5.4.11 (tar.bz2)</name>
      <filename>php-5.4.11.tar.bz2</filename>
      <date>2013-01-17</date>
      <md5>9975e68c22b86b013b934743ad2d2276</md5>
    </file>
    <file compression="tar.gz">
      <name>PHP 5.4.11 (tar.gz)</name>
      <filename>php-5.4.11.tar.gz</filename>
      <date>2013-01-17</date>
      <md5>32fa16b3abd5527316c3c076b3395914</md5>
    </file>
  </source>
  <announcements>
    <announcement lang="en"/>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(Sigsegv when Exception::$trace is changed by user).</description>
          <references>
            <reference type="bugfix">63762</reference>
          </references>
        </change>
        <change>
          <description>(Errors in eval()'ed code produce status code 500).</description>
          <references>
            <reference type="bugfix">43177</reference>
          </references>
        </change>
      </module>
      <module id="filter">
        <change>
          <description>(getenv() produces memory leak with CGI SAPI).</description>
          <references>
            <reference type="bugfix">63757</reference>
          </references>
        </change>
        <change>
          <description>(FILTER_VALIDATE_INT does not accept +0 and -0).</description>
          <references>
            <reference type="bugfix">54096</reference>
          </references>
        </change>
      </module>
      <module id="json">
        <change>
          <description>(json_decode does not properly decode with options parameter).</description>
          <references>
            <reference type="bugfix">63737</reference>
          </references>
        </change>
      </module>
      <module id="cli server">
        <change>
          <description>Update list of common mime types. Added webm, ogv, ogg.</description>
          <references/>
        </change>
      </module>
      <module id="curl extension">
        <change>
          <description>Fixed bug (segfault due to libcurl connection caching).</description>
          <references/>
        </change>
        <change>
          <description>(Memory leak when reusing curl-handle).</description>
          <references>
            <reference type="bugfix">63859</reference>
          </references>
        </change>
        <change>
          <description>(CURL &gt;= 7.28.0 no longer support value 1 for CURLOPT_SSL_VERIFYHOST).</description>
          <references>
            <reference type="bugfix">63795</reference>
          </references>
        </change>
        <change>
          <description>(Can't enable hostname validation when using curl stream wrappers).</description>
          <references>
            <reference type="bugfix">63352</reference>
          </references>
        </change>
        <change>
          <description>(Curlwapper is not sending http header randomly).</description>
          <references>
            <reference type="bugfix">55438</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <windows_builds/>
  <tags/>
</release>
