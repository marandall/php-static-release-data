<?xml version="1.0"?>
<release version="5.4.23">
  <version>5.4.23</version>
  <date>2013-12-12</date>
  <stage>stable</stage>
  <source>
    <file compression="tar.bz2">
      <name>PHP 5.4.23 (tar.bz2)</name>
      <filename>php-5.4.23.tar.bz2</filename>
      <date>2013-12-12</date>
      <sha256>ae7c070fa9b9e16413ef944d910b68f3ba79192eca4010b0af132b8631bd91cc</sha256>
    </file>
    <file compression="tar.gz">
      <name>PHP 5.4.23 (tar.gz)</name>
      <filename>php-5.4.23.tar.gz</filename>
      <date>2013-12-12</date>
      <sha256>c9add0e59f41298a253bbb9090c47a03064b099120a563ca8ad289e18fcd1ce7</sha256>
    </file>
  </source>
  <announcements>
    <announcement lang="en"/>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(unregister_tick_function tries to cast a Closure to a string).</description>
          <references>
            <reference type="bugfix">66094</reference>
          </references>
        </change>
        <change>
          <description>(basename is no more working after fgetcsv in certain situation).</description>
          <references>
            <reference type="bugfix">65947</reference>
          </references>
        </change>
      </module>
      <module id="json">
        <change>
          <description>Fixed whitespace part of [#64874](http://bugs.php.net/64874) ("json_decode handles whitespace and case-sensitivity incorrectly").</description>
          <references/>
        </change>
      </module>
      <module id="mysqli">
        <change>
          <description>(Segfault calling bind_param() on mysqli).</description>
          <references>
            <reference type="bugfix">66043</reference>
          </references>
        </change>
      </module>
      <module id="mysqlnd">
        <change>
          <description>(mysqli under mysqlnd loses precision when bind_param with 'i').</description>
          <references>
            <reference type="bugfix">66124</reference>
          </references>
        </change>
        <change>
          <description>(mysqlnd quote function is wrong with NO_BACKSLASH_ESCAPES after failed query).</description>
          <references>
            <reference type="bugfix">66141</reference>
          </references>
        </change>
      </module>
      <module id="openssl">
        <change>
          <description>Fixed memory corruption in openssl_x509_parse()  (Stefan Esser).</description>
          <references>
            <reference type="cve">CVE-2013-6420</reference>
          </references>
        </change>
      </module>
      <module id="pdo">
        <change>
          <description>(sql_parser permanently converts values bound to strings).</description>
          <references>
            <reference type="bugfix">65946</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <windows_builds/>
  <tags/>
</release>
