      <module id="wddx">
        <change>
          <description>(Segfaults in php_wddx_serialize_var).</description>
          <references>
            <reference type="bugfix">67873</reference>
          </references>
        </change>
      </module>
      <module id="zlib">
        <change>
          <description>(chained zlib filters silently fail with large amounts of data).</description>
          <references>
            <reference type="bugfix">67724</reference>
          </references>
        </change>
        <change>
          <description>(internal corruption phar error).</description>
          <references>
            <reference type="bugfix">67865</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
