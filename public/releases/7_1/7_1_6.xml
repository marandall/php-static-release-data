  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(crash (SIGSEGV) in _zend_hash_add_or_update_i).</description>
          <references>
            <reference type="bugfix">74600</reference>
          </references>
        </change>
        <change>
          <description>(SIGILL in ZEND_FETCH_CLASS_CONSTANT_SPEC_CONST_CONST).</description>
          <references>
            <reference type="bugfix">74546</reference>
          </references>
        </change>
        <change>
          <description>(__DIR__ wrong for unicode character).</description>
          <references>
            <reference type="bugfix">74589</reference>
          </references>
        </change>
      </module>
      <module id="intl">
        <change>
          <description>(wrong reflection on Collator::sortWithSortKeys).</description>
          <references>
            <reference type="bugfix">74468</reference>
          </references>
        </change>
      </module>
      <module id="mysqli">
        <change>
          <description>(mysqli::change_user() doesn't accept null as $database argument w/strict_types).</description>
          <references>
            <reference type="bugfix">74547</reference>
          </references>
        </change>
      </module>
      <module id="opcache">
        <change>
          <description>(SIGSEGV with opcache.revalidate_path enabled).</description>
          <references>
            <reference type="bugfix">74596</reference>
          </references>
        </change>
      </module>
      <module id="phar">
        <change>
          <description>(Phar::webPhar() does not handle requests sent through PUT and DELETE method).</description>
          <references>
            <reference type="bugfix">51918</reference>
          </references>
        </change>
      </module>
      <module id="readline">
        <change>
          <description>(readline() moves the cursor to the beginning of the line).</description>
          <references>
            <reference type="bugfix">74490</reference>
          </references>
        </change>
      </module>
      <module id="standard">
        <change>
          <description>(win32/sendmail.c anchors CC header but not BCC).</description>
          <references>
            <reference type="bugfix">74510</reference>
          </references>
        </change>
      </module>
      <module id="xmlreader">
        <change>
          <description>(Wrong reflection on XMLReader::expand).</description>
          <references>
            <reference type="bugfix">74457</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
