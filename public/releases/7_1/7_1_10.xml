  <announcements>
    <announcement lang="en"/>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(run-tests.php issues with EXTENSION block).</description>
          <references>
            <reference type="bugfix">75042</reference>
          </references>
        </change>
      </module>
      <module id="bcmath">
        <change>
          <description>(bcpowmod() fails if scale != 0).</description>
          <references>
            <reference type="bugfix">44995</reference>
          </references>
        </change>
        <change>
          <description>(BC math handles minus zero incorrectly).</description>
          <references>
            <reference type="bugfix">46781</reference>
          </references>
        </change>
        <change>
          <description>(bcpowmod() may return 1 if modulus is 1).</description>
          <references>
            <reference type="bugfix">54598</reference>
          </references>
        </change>
        <change>
          <description>(bcpowmod() misbehaves for non-integer base or modulus).</description>
          <references>
            <reference type="bugfix">75178</reference>
          </references>
        </change>
      </module>
      <module id="cli server">
        <change>
          <description>(Built-in server truncates headers spanning over TCP packets).</description>
          <references>
            <reference type="bugfix">70470</reference>
          </references>
        </change>
      </module>
      <module id="curl">
        <change>
          <description>(OpenSSL support not detected).</description>
          <references>
            <reference type="bugfix">75093</reference>
          </references>
        </change>
      </module>
      <module id="gd">
        <change>
          <description>(gdImageGrayScale() may produce colors).</description>
          <references>
            <reference type="bugfix">75124</reference>
          </references>
        </change>
        <change>
          <description>(libgd/gd_interpolation.c:1786: suspicious if ?).</description>
          <references>
            <reference type="bugfix">75139</reference>
          </references>
        </change>
      </module>
      <module id="gettext">
        <change>
          <description>(textdomain(null) throws in strict mode).</description>
          <references>
            <reference type="bugfix">73730</reference>
          </references>
        </change>
      </module>
      <module id="intl">
        <change>
          <description>(IntlGregorianCalendar doesn't have constants from parent class).</description>
          <references>
            <reference type="bugfix">75090</reference>
          </references>
        </change>
        <change>
          <description>(segfault in collator_convert_object_to_string).</description>
          <references>
            <reference type="bugfix">75193</reference>
          </references>
        </change>
      </module>
      <module id="pdo_oci">
        <change>
          <description>(PDO_PCO with PHP-FPM: OCI environment initialized before PHP-FPM sets it up).</description>
          <references>
            <reference type="bugfix">74631</reference>
          </references>
        </change>
      </module>
      <module id="spl">
        <change>
          <description>(AppendIterator::append() is broken when appending another AppendIterator).</description>
          <references>
            <reference type="bugfix">75155</reference>
          </references>
        </change>
        <change>
          <description>(incorrect behavior of AppendIterator::append in foreach loop).</description>
          <references>
            <reference type="bugfix">75173</reference>
          </references>
        </change>
      </module>
      <module id="standard">
        <change>
          <description>(signed integer overflow in parse_iv).</description>
          <references>
            <reference type="bugfix">75152</reference>
          </references>
        </change>
        <change>
          <description>(gethostname fails if your host name is 64 chars long).</description>
          <references>
            <reference type="bugfix">75097</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <windows_builds/>
  <tags/>
</release>
