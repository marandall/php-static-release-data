    <source>
      <name>PHP 7.1.23 (tar.xz)</name>
      <filename>php-7.1.23.tar.xz</filename>
      <date>2018-10-11</date>
      <sha256>227a3c76133c3dc1cec937989456cbd89ed00e68e7260c651900dbe1f5b798bc</sha256>
