<?xml version="1.0"?>
<release version="7.3.10">
  <version>7.3.10</version>
  <date>2019-09-26</date>
  <stage>stable</stage>
  <source>
    <file compression="tar.bz2">
      <name>PHP 7.3.10 (tar.bz2)</name>
      <filename>php-7.3.10.tar.bz2</filename>
      <date>2019-09-26</date>
      <sha256>506dd871c0fb8f00f872f53dd3b1dfa5f23a9edb4dfc521e5669c78a78c45448</sha256>
    </file>
    <file compression="tar.gz">
      <name>PHP 7.3.10 (tar.gz)</name>
      <filename>php-7.3.10.tar.gz</filename>
      <date>2019-09-26</date>
      <sha256>fb670723a9b8fda31c89529f27e0dda289d8af4b6ce9f152c8010876639c0fb4</sha256>
    </file>
    <file compression="tar.xz">
      <name>PHP 7.3.10 (tar.xz)</name>
      <filename>php-7.3.10.tar.xz</filename>
      <date>2019-09-26</date>
      <sha256>42f00a15419e05771734b7159c8d39d639b8a5a6770413adfa2615f6f923d906</sha256>
    </file>
  </source>
  <announcements>
    <announcement lang="en">The PHP development team announces the immediate availability of PHP 7.3.10. This is a security release which also contains several bug fixes.</announcement>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(Can't access OneDrive folder).</description>
          <references>
            <reference type="bugfix">78220</reference>
          </references>
        </change>
        <change>
          <description>(Double release of doc comment on inherited shadow property).</description>
          <references>
            <reference type="bugfix">77922</reference>
          </references>
        </change>
        <change>
          <description>(Parse error due to heredoc identifier followed by digit).</description>
          <references>
            <reference type="bugfix">78441</reference>
          </references>
        </change>
        <change>
          <description>(Interactive mode does not support PHP 7.3-style heredoc).</description>
          <references>
            <reference type="bugfix">77812</reference>
          </references>
        </change>
      </module>
      <module id="fastcgi">
        <change>
          <description>(FastCGI on_accept hook is not called when using named pipes on Windows).</description>
          <references>
            <reference type="bugfix">78469</reference>
          </references>
        </change>
      </module>
      <module id="fpm">
        <change>
          <description>(fpm log prefix message includes wrong stdout/stderr notation).</description>
          <references>
            <reference type="bugfix">78334</reference>
          </references>
        </change>
      </module>
      <module id="intl">
        <change>
          <description>Ensure IDNA2003 rules are used with idn_to_ascii() and idn_to_utf8() when requested.</description>
          <references/>
        </change>
      </module>
      <module id="mbstring">
        <change>
          <description>(Heap buffer overflow in mb_eregi).</description>
          <references>
            <reference type="bugfix">78559</reference>
          </references>
        </change>
      </module>
      <module id="mysqlnd">
        <change>
          <description>Fixed connect_attr issues and added the _server_host connection attribute.</description>
          <references/>
        </change>
      </module>
      <module id="odbc">
        <change>
          <description>(odbc_close() closes arbitrary resources).</description>
          <references>
            <reference type="bugfix">78473</reference>
          </references>
        </change>
      </module>
      <module id="pdo_mysql">
        <change>
          <description>(SP call yields additional empty result set).</description>
          <references>
            <reference type="bugfix">41997</reference>
          </references>
        </change>
      </module>
      <module id="sodium">
        <change>
          <description>(Partially uninitialized buffer returned by sodium_crypto_generichash_init()).</description>
          <references>
            <reference type="bugfix">78510</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <windows_builds/>
  <tags>
    <tag>security</tag>
  </tags>
</release>
